{
  "name": "hashtag-protocol-contracts",
  "version": "1.0.0",
  "description": "hashtag-protocol-contracts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "node:hardhat": "hardhat node --no-deploy",
    "node:ganache": "$PWD/scripts/node-ganache.sh",
    "test:hardhat": "hardhat test --network localhost",
    "test:ganache": "hardhat test --network ganache",
    "deploy:hardhat": "hardhat deploy --network localhost --tags dev,Ethernal",
    "deploy:ganache": "hardhat deploy --network ganache --tags dev",
    "deploy:ethernal": "hardhat deploy --network localhost --tags Ethernal",
    "deploy:mumbai": "hardhat deploy --network mumbai --tags dev"
  },
  "author": "Hashtag Protocol, Inc.",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-truffle5": "link:../../hardhat/packages/hardhat-truffle5",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "@openzeppelin/test-helpers": "^0.5.13",
    "chai": "^4.2.0",
    "dotenv": "^9.0.2",
    "ethereum-waffle": "^3.1.1",
    "ethers": "^5.4.4",
    "hardhat": "^2.6.4",
    "hardhat-deploy": "^0.9.1",
    "hardhat-ethernal": "^0.3.10",
    "web3": "^1.3.5"
  }
}
